			<ROOT>
				<TCEforms>
					<sheetTitle>Angaben zum Hintergrundbild</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<image>
						<TCEforms>
							<label>Hintergrundbild für den Adventskalender</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>gif,png,jpg,jpeg</allowed>
								<show_thumbs>1</show_thumbs>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>1</minitems>
							</config>
						</TCEforms>
					</image>
					<imageWidth>
						<TCEforms>
							<label>Bildbreite in px</label>
							<config>
								<type>input</type>
								<eval>text</eval>
								<size>5</size>
							</config>
						</TCEforms>
					</imageWidth>
					<imageHeight>
						<TCEforms>
							<label>Bildhöhe in px</label>
							<config>
								<type>input</type>
								<eval>text</eval>
								<size>5</size>
							</config>
						</TCEforms>
					</imageHeight>
					<altText>
						<TCEforms>
							<label>Alternative Text</label>
							<config>
								<type>input</type>
								<eval>text</eval>
								<size>25</size>
							</config>
						</TCEforms>
					</altText>
				</el>
			</ROOT>
		</sDEF>
